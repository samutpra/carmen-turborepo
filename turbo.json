{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [".env"],
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**", ".dist/**", "!.dist/cache/**", "dist/**", "**/dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
