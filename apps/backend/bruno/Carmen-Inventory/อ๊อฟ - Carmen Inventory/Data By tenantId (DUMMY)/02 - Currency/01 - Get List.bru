meta {
  name: 01 - Get List
  type: http
  seq: 1
}

get {
  url: {{host}}/api/v1/currencies?sort=code,name:asc&searchfields=code,name,description,is_active:bool
  body: none
  auth: bearer
}

params:query {
  sort: code,name:asc
  searchfields: code,name,description,is_active:bool
  ~page: 1
  ~filter[isActive: bool]: 1
  ~filter[code]: us
  ~search: baht
  ~advance: {'where':{'code':'usd'}}
  ~perpage: 2
}

headers {
  x-tenant-id: {{tenantId}}
}

auth:bearer {
  token: {{access_token}}
}
